{
  "manifest_version": 2,

  "name": "ProductivityExtension",
  "description": "Increase your productivity.",
  "version": "1.0",


"background": {"scripts":["popup.js","blocking.js"]},

"options_page": "options.html",


  "permissions": [
          "tabs","topSites","webRequest","storage",
          "webRequestBlocking",
          "*://*/*"
        ],



        

  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },


  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  }
}
